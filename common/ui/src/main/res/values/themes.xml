<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.Globe" parent="Theme.MaterialComponents.Light">
        <item name="colorPrimary">@color/green_primary</item>
        <item name="colorPrimaryVariant">@color/green_primary_variant</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorSecondary">@color/blue_primary</item>
        <item name="colorSecondaryVariant">@color/blue_secondary</item>
        <item name="colorOnSecondary">@color/white</item>
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
    </style>

    <style name="Theme.Globe.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Globe.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Globe.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>